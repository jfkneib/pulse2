# Installer le serveur SSH du module LSC et la cle id_dsa.pub et VNC
# desc_en: Install the LSC SSH service and the id_dsa.pub key and VNC
Mount 1

mkdir -p /mnt/tmp

cp /opt/winutils/setupssh.exe /mnt/tmp
cp /opt/winutils/tightvncssh.exe /mnt/tmp
cp /revoinfo/data/id_dsa.pub /mnt

echo "cd c:\tmp
setupssh.exe /S
tightvncssh.exe /sp- /silent /norestart
del c:\tmp\setupssh.exe" | unix2dos > /mnt/tmp/setup.bat

RegistryAddRunOnce 'c:\tmp\setup.bat'
